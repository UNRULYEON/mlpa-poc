		"@prisma/client": "2.22.1",
		"@pulumi/azure-native": "^1.6.0",
		"@pulumi/gcp": "^5.0.0",
		"@pulumi/pulumi": "^3.1.0",
		"dotenv": "^10.0.0",
		"express": "^4.17.1",
		"multer": "^1.4.2",
		"prisma": "2.22.1"
	},
	"devDependencies": {
		"@types/express": "^4.17.11",
		"@types/multer": "^1.4.5",
		"@types/node": "^15.0.2",
		"nodemon": "^2.0.7",
